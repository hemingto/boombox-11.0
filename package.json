{
  "name": "boombox-11.0",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "db:seed": "node prisma/seed.cjs",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "test": "NODE_OPTIONS='--max_old_space_size=4096' jest",
    "test:watch": "NODE_OPTIONS='--max_old_space_size=4096' jest --watch",
    "test:components": "jest --testPathPatterns=tests/components",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "migration:analyze": "node tools/api-migration-analyzer.js analyze-domain",
    "migration:check-duplicates": "node tools/api-migration-analyzer.js check-duplicates",
    "migration:suggest-consolidation": "node tools/api-migration-analyzer.js suggest-consolidation",
    "utils:scan-duplicates": "node tools/utility-duplication-scanner.js scan",
    "utils:detailed-report": "node tools/utility-duplication-scanner.js detailed-report",
    "refactor:scan": "node tools/refactor-scanner.js scan",
    "refactor:ready": "node tools/refactor-scanner.js ready",
    "refactor:update": "node tools/refactor-scanner.js update",
    "refactor:report": "node tools/refactor-scanner.js scan",
    "refactor:validate": "npm run lint && npm run type-check && npm run refactor:scan",
    "components:pre-batch": "npm run utils:scan-duplicates",
    "components:post-batch": "npm run utils:scan-duplicates"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@hcaptcha/react-hcaptcha": "^1.12.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@onfleet/node-onfleet": "^1.3.8",
    "@prisma/client": "^6.11.1",
    "@react-google-maps/api": "^2.20.7",
    "@sendgrid/mail": "^8.1.5",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "hcaptcha": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "15.3.6",
    "next-auth": "^4.24.11",
    "openai": "^5.8.3",
    "random-word-slugs": "^0.1.7",
    "react": "^19.0.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.7.2",
    "uuid": "^11.1.0",
    "web-vitals": "^5.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.2",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-links": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/addon-vitest": "^9.0.17",
    "@storybook/nextjs-vite": "^9.0.17",
    "@storybook/react": "^9.0.17",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/onfleet__node-onfleet": "^1.3.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.24.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-storybook": "^9.0.17",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.2",
    "playwright": "^1.54.1",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "storybook": "^9.0.17",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
